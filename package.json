{
  "name": "ts-dax",
  "version": "0.0.1",
  "description": "Data Analysis Expression (DAX) functions implemented in TypeScript",
  "keywords": [
    "dax"
  ],
  "homepage": "https://github.com/lukecarr/ts-dax#readme",
  "bugs": "https://github.com/lukecarr/ts-dax/issues",
  "repository": "lukecarr/ts-dax",
  "author": "Luke Carr <me+oss@carr.sh>",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "fmt": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "5.1.1",
    "@vitest/coverage-istanbul": "0.24.4",
    "dprint": "0.32.2",
    "husky": "8.0.1",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "release-it": "15.5.0",
    "tsup": "6.3.0",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vitest": "0.24.4"
  },
  "nano-staged": {
    "*.{js,ts,md,json,yaml,yml}": "dprint fmt"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true
  },
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "packageManager": "pnpm@7.14.1"
}
